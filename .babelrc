{
    "presets": [
        "react",
        "es2015",
        "stage-1"
    ],
    "plugins": [
        "transform-decorators-legacy",
        "transform-object-rest-spread",
        [
            "transform-runtime", {
            "regenerator": true,
            "moduleName": "babel-runtime"
        }
        ],
        [
            "import", {
            "libraryName": "antd",
            "style": true
        },
            "antd"
        ]
    ]
}
